<h2>Favorites Page</h2>
<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">_id</th>
            <th scope="col">Headline</th>
            <th scope="col">Summary</th>
            <th scope="col">Link</th>
        </tr>
    </thead>
    <tbody>
        {{#each movie}}
        <tr>
            <td>{{this._id}}</td>
            <td>{{this.headline}}</td>
            <td>{{this.summary}}</td>
            <td>{{this.link}}/10</td>
            <td>
                <button class="movie-unfavorite" data-id={{this._id}}>Unfavorite Movie</button>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>